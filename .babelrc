{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "8.17.0"
        },
        "loose": true,
        "modules": "commonjs"
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-export-default-from"
  ],
  "sourceMaps": true
}